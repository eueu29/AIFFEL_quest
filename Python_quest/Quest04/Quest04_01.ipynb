        "        print(\"연산결과 : \", calc(),\"\\n\")\n",
        "    elif a == 'n':\n",
        "        print(\"계산을 종료합니다.\")\n",
        "        break\n",
        "    else:\n",
        "        print(\"잘못된 입력입니다.\")"
